<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<FlakeWombat.AmmoSubTypeDef Name="FW_JNK">
		<defName>JNK</defName>
		<label>junk ammo</label>

		<level>Industrial</level>
		<damageMult>0.8</damageMult>
		<armorPenMult>0.8</armorPenMult>
	</FlakeWombat.AmmoSubTypeDef>

	<ThingDef ParentName="FW_Ammobase" Name="FW_AmmobaseJunk" Abstract="True">
		<statBases>
			<MarketValue>0</MarketValue>
		</statBases>
		<recipeMaker>
			<workAmount>1600</workAmount>
			<recipeUsers Inherit="False">
				<li>CraftingSpot</li>
			</recipeUsers>
		</recipeMaker>
		<tradeTags Inherit="False" />
	</ThingDef>

	<ThingDef ParentName="FW_AmmobaseJunk">
		<defName>FW_RoundIndustrialPistolJNK</defName>
		<label>junk pistol round</label>
		<description>A crude, full metal jacket, low pressure round.</description>
		<graphicData>
			<texPath>Ammo/Pistol_Industrial</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>0.90</drawSize>
			<color>(196, 150, 109)</color>
		</graphicData>
		<stackLimit>500</stackLimit>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<defaultStuff>WoodLog</defaultStuff>
		<costStuffCount>15</costStuffCount>
		<costList>
			<Steel>15</Steel>
		</costList>
		<recipeMaker>
			<productCount>100</productCount>
			<bulkRecipeCount>5</bulkRecipeCount>
		</recipeMaker>
	</ThingDef>
	<ThingDef ParentName="FW_AmmobaseJunk">
		<defName>FW_RoundIndustrialRifleJNK</defName>

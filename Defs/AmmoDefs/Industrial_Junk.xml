	<ThingDef ParentName="FW_AmmobaseJunk">
		<defName>FW_RoundIndustrialShotgunJNK</defName>
		<label>junk shotgun shell</label>
		<description>A crude shotgun shell containing heavy buckshot.</description>
		<graphicData>
			<texPath>Ammo/Shotgun_Industrial</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>0.90</drawSize>
			<color>(196, 150, 109)</color>

<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<FlakeWombat.AmmoSubTypeDef Name="FW_HP">
		<defName>HollowPoint</defName>
		<label>hollow point rounds</label>

		<level>Industrial</level>
		<type>Pistol, Rifle</type>

		<damageMult>1.35</damageMult>
		<armorPenMult>0</armorPenMult>
	</FlakeWombat.AmmoSubTypeDef>

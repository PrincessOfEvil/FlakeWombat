	<ThingDef ParentName="FW_Ammobase">
		<defName>FW_RoundIndustrialPistolHollowPoint</defName>
		<label>hollow point pistol round</label>
		<description>A simple, hollow point, low pressure round. Does less damage to armored targets, but much more to unarmored ones.</description>
		<graphicData>
			<texPath>Ammo/Pistol_Industrial</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>0.90</drawSize>
			<color>(192, 192, 0)</color>
		</graphicData>
		<stackLimit>500</stackLimit>
		<costList>
			<Steel>15</Steel>
			<Chemfuel>15</Chemfuel>
		</costList>
		<recipeMaker>
			<workAmount>1000</workAmount>
			<productCount>100</productCount>
			<bulkRecipeCount>5</bulkRecipeCount>
		</recipeMaker>
	</ThingDef>

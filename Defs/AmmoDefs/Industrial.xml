<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="FW_Ammobase">
		<defName>FW_RoundIndustrialPistol</defName>
		<label>pistol round</label>
		<description>A simple, full metal jacket, low pressure round.</description>
		<graphicData>
			<texPath>Ammo/Pistol_Industrial</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>0.90</drawSize>
			<color>(186, 140, 99)</color>
		</graphicData>
		<stackLimit>500</stackLimit>
		<costList>
			<Steel>15</Steel>
			<Chemfuel>15</Chemfuel>
		</costList>
		<recipeMaker>
			<productCount>100</productCount>
			<bulkRecipeCount>5</bulkRecipeCount>
		</recipeMaker>
		<tradeTags>
			<li>FW_Ammo_Industrial_Basic</li>
		</tradeTags>
	</ThingDef>

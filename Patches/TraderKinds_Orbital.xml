<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods"]/stockGenerators</xpath>
				<value>
					<li Class="FlakeWombat.StockGenerator_Tag_IgnoreStuff">
						<tradeTag>FW_Ammo_Industrial_Basic</tradeTag>
						<thingDefCountRange>5~5</thingDefCountRange>
						<totalPriceRange>2000~3000</totalPriceRange>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods"]/stockGenerators</xpath>
				<value>
					<li Class="FlakeWombat.StockGenerator_Tag_IgnoreStuff">
						<tradeTag>FW_Ammo_Spacer_Basic</tradeTag>
						<thingDefCountRange>5~5</thingDefCountRange>
						<totalPriceRange>2000~3000</totalPriceRange>
					</li>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/TraderKindDef[defName="Orbital_CombatSupplier" or defName="Orbital_PirateMerchant"]/stockGenerators</xpath>
				<value>
					<li Class="FlakeWombat.StockGenerator_Tag_IgnoreStuff">
						<tradeTag>FW_Ammo_Industrial</tradeTag>
						<thingDefCountRange>4~8</thingDefCountRange>
						<totalPriceRange>2000~3000</totalPriceRange>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TraderKindDef[defName="Orbital_CombatSupplier" or defName="Orbital_PirateMerchant"]/stockGenerators</xpath>
				<value>
					<li Class="FlakeWombat.StockGenerator_Tag_IgnoreStuff">
						<tradeTag>FW_Ammo_Spacer</tradeTag>
						<thingDefCountRange>4~8</thingDefCountRange>
						<totalPriceRange>2000~3000</totalPriceRange>
					</li>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods" or defName="Orbital_CombatSupplier" or defName="Orbital_PirateMerchant"]/stockGenerators</xpath>
				<value>
					<li Class="StockGenerator_BuyTradeTag"><tag>FW_Ammo</tag></li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>